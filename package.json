{
  "name": "fetch-factory",
  "version": "0.2.1",
  "description": "Easy JS objects for talking to your APIs",
  "main": "lib/index.js",
  "scripts": {
    "example": "open http://localhost:8000/example && python -m SimpleHTTPServer",
    "browserify": "mkdir -p dist && browserify lib/browserify-entry.js -o dist/fetch-factory.js -s fetchFactory",
    "minify": "uglifyjs dist/fetch-factory.js > dist/fetch-factory.min.js",
    "build": "npm run babel && npm run browserify && npm run minify",
    "babel": "babel -d lib src",
    "prepublish": "npm run build",
    "pretest": "eslint src spec",
    "test": "babel-tape-runner spec/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jackfranklin/fetch-factory.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "es6-promise": "^3.0.2",
    "isomorphic-fetch": "^2.1.1",
    "lodash": "^3.10.1",
    "query-string": "^2.4.1",
    "url-pattern": "^0.10.0"
  },
  "devDependencies": {
    "babel-cli": "^6.1.2",
    "babel-core": "^6.1.2",
    "babel-eslint": "^4.1.6",
    "babel-preset-es2015": "^6.1.2",
    "babel-tape-runner": "^1.3.0",
    "browserify": "^12.0.1",
    "eslint": "^1.10.3",
    "nock": "^2.17.0",
    "sinon": "^1.16.1",
    "tape": "^4.2.0",
    "uglify-js": "^2.5.0"
  }
}
